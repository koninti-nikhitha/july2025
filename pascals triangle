# given row,colum tell the element at that place

def ncr(n, r):
    if r > n:
        return 0
    if r == 0 or r == n:
        return 1
    res = 1
    for i in range(1, r + 1):
        res = res * (n - i + 1) // i
    return res
n=5
k=3
print(ncr(n,r))
output:6
---------------------------------------
#print any with nth row output
class Solution:
    def getRow(self, rowIndex: int) -> List[int]:
        res=1
        ans=[1]
        for i in range(1,rowIndex+1):
            res=res*(rowIndex-i+1)//i
            ans.append(res)
        return ans
