# missing and repeating brute force
arr=[1,2,2,3,4,6]
n=len(arr)
repeating=0
missing=0
for i in range(1,n+1): #elements form 1 to n not in array
    count=0
    for j in range(n):#array elements
        if arr[j]==i:
            count+=1
if (count==2):
    repeating=i

elif (count==0):
    missing=i
print(repeating)
print(missing)


arr = [1, 2, 2, 3, 4, 6]
n = len(arr)

repeating = 0
missing = 0
hasharr = [0] * (n + 1)  # Index from 1 to n

# Fill hasharr with counts of each number in arr
for num in arr:
    hasharr[num] += 1

# Now find repeating and missing
for i in range(1, n + 1):
    if hasharr[i] == 0:
        missing = i
    elif hasharr[i] == 2:
        repeating = i

print("Missing:", missing)
print("Repeating:", repeating)
